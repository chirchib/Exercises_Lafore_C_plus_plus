#include <iostream>
#include <ctype.h>
using namespace std;

int main()
{
  double dollar; cin >> dollar;
  cout << dollar * 1.487 << " pounds" << endl;
  cout << dollar * 0.172 << " francs" << endl;
  cout << dollar * 0.584 << " marks" << endl;
  cout << dollar * 0.00955 << " yen" << endl;
  
  return 0;
}
